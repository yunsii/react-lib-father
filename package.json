{
  "name": "react-lib-father",
  "scripts": {
    "init": "rm -rf node_modules && cnpm i && npm run clean && npm run bootstrap && npm run build",
    "start": "npm run dev",
    "dev": "dumi dev",
    "bootstrap": "lerna bootstrap",
    "clean": "lerna clean --yes",
    "build:umd": "lerna run build:umd",
    "build": "node ./scripts/clean-old-build.js && father build && npm run build:umd && node ./scripts/build-wind-up.js",
    "build:doc": "dumi build",
    "pub": "lerna publish"
  },
