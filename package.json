  },
  "homepage": "https://github.com/zpr1g/react-lib-father#readme",
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "babel-loader": "^8.1.0",
    "babel-plugin-import": "^1.13.0",
    "babel-plugin-transform-async-to-promises": "^0.8.15",
    "del": "^5.1.0",
    "dumi": "^1.0.31",
    "father": "^2.29.3",
    "fork-ts-checker-webpack-plugin": "^4.1.3",
